<div id="content-header">
    <img src="/images/show_finance.png" alt="" />
  <h1><%= @student.first_name+" "+@student.last_name%></h1>
  <h3><%= t('app.views.finance.fees_student_dates.title') %></h3>
  <div id="app-back-button">
    <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
  </div>
</div>

<div id="page-yield">
  <div class="label-field-pair">
    <label><%=t('app.views.finance.fees_student_dates.select_fee_collection') %></label>
  <div class="text-input-bg">
    <%= select  :fees_submission, :dates_id, @dates.map { |e| [e.full_name, e.id]},
      {:prompt => t('app.views.finance.fees_student_dates.select_fee_collection_')},
      {:onChange => "#{remote_function( :url =>
      {:action => "fees_submission_student"},
      :with => "'date='+value+'&id='+#{@student.id}") }"} %>
  </div>
    </div>
      </div>
<div id= "fee_submission">

</div>

<div id="modal-box" style="display:none;"></div>