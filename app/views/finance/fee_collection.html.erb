<div id="content-header">
    <img src="/images/show_finance.png" alt="" />
    <h1><%= t('app.views.finance.fee_collection.finance') %></h1>
    <h3><%= t('app.views.finance.fee_collection.fee_collection_dates') %></h3>
  <div id="app-back-button">
    <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
  </div>
</div>
<div id="page-yield">
  <div class="box">
  <div id ="message"></div>
  <div class ="link-box">
    <div class="link-heading"> 
      <%= link_to_remote t('app.views.finance.fee_collection.create_fee_collection'), :url => { :action => 'fee_collection_new'} %>
    </div>
    <div class="link-descr"><%= t('app.views.finance.fee_collection.create_fee_collection_desc') %></div>
  </div>
  <div class="link-box">
    <div class="link-heading"><%= link_to t('app.views.finance.fee_collection.view'),:action => "fee_collection_view" %></div>
    <div class="link-descr"><%= t('app.views.finance.fee_collection.view_desc') %></div>
  </div>

  <div id="modal-box" style="display:none;"></div>
</div>
</div>
