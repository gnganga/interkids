<div id="content-header">
   <img src="/images/show_finance.png" alt="" />
  <h1><%=t('app.views.finance.expense_create.create_expense')%></h1>
  <h3><%=t('app.views.finance.expense_create.manage_transactions')%></h3>
  <div id="app-back-button">
    <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
  </div>
</div>

<div id="page-yield">
  <% unless flash[:notice].nil? %>
  <p class="flash-msg"><%= flash[:notice] %>  </p>
<% end %>

  <% unless flash[:maximum].nil? %>
  <p class="flash-msg"><%= flash[:maximum] %></p>
  <% end %>

  <% unless flash[:minimum].nil? %>
  <p class="flash-msg"><%= flash[:minimum] %></p>
  <% end %>

    <div class="box">
      <%=  error_messages_for :expense %>
  <%= render :partial => "transaction_create_form" %>
</div>
</div>
