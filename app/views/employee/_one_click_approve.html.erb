
<%unless @dates.empty? %>
  <div id="submit-button" class ="approve">
      <%= link_to t('app.views.employee._one_click_approve.approve'), {:controller=>"employee", :action=>"one_click_approve_submit",:date => @salary_date } %>
  </div>
<% else%>
  <div id="submit-button" class ="approved">
     <%= t('app.views.employee._one_click_approve.already_approved') %>
  </div>
<% end %>



